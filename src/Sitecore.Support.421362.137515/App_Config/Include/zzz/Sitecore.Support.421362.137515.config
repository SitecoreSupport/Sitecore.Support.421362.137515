<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/">
  <sitecore database="SqlServer">
    <archives defaultProvider="sql" enabled="true">
      <providers>
        <add name="sql" type="Sitecore.Support.Data.Archiving.SqlArchiveProvider" database="*" patch:instead="*[@type='Sitecore.Data.Archiving.SqlArchiveProvider, Sitecore.Kernel']" />
      </providers>
    </archives>
    <pipelines>
      <initialize>
        <processor type="Sitecore.Pipelines.Loader.ShowVersion, Sitecore.Kernel">
          <assemblies>
            <assembly id="Sitecore.Support.421362.137515">/bin/Sitecore.Support.421362.137515.dll</assembly>
          </assemblies>
        </processor>
      </initialize>
    </pipelines>
  </sitecore>
</configuration>